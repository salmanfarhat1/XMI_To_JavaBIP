<?xml version="1.0" encoding="UTF-8"?>
<Feature_model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://fr.wikipedia.org/wiki/Feature_model"
    xsi:schemaLocation="https://fr.wikipedia.org/wiki/Feature_model ../MetaModels/Feature_MM.ecore">
  <root name="Module">
    <SubFeatures
        name="Radar">
      <SubFeatures
          xsi:type="Exclusive"
          name="SpinningRadar"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="TheInfinityLockRadar"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="WidthLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="TurnMultiplierLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="LazyLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="SpotlightLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="FancyRadar"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="FactorLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="InteractiveRadar">
        <SubFeatures
            name="MouseRadar"
            optional="true"/>
      </SubFeatures>
    </SubFeatures>
    <SubFeatures
        name="Movement">
      <SubFeatures
          xsi:type="Exclusive"
          name="BasicMovement">
        <SubFeatures
            xsi:type="Exclusive"
            name="WallsMovement">
          <SubFeatures
              xsi:type="Exclusive"
              name="WallsMovementWithWallSmoothing"/>
          <SubFeatures
              xsi:type="Exclusive"
              name="BasicWallsMovement"/>
        </SubFeatures>
        <SubFeatures
            xsi:type="Exclusive"
            name="RandomMovement">
          <SubFeatures
              xsi:type="Exclusive"
              name="BasicRandomMovement"/>
          <SubFeatures
              xsi:type="Exclusive"
              name="TimeBasedRandomMovement"/>
        </SubFeatures>
      </SubFeatures>
      <SubFeatures
          xsi:type="Exclusive"
          name="AdvancedMovement"/>
    </SubFeatures>
    <SubFeatures
        name="Targeting">
      <SubFeatures
          xsi:type="Exclusive"
          name="BasicTargeting">
        <SubFeatures
            xsi:type="Exclusive"
            name="HeadOnTargeting"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="LinearTargeting"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="CircularTargeting"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="PredictiveTargeting"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="RandomTargeting"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="NoTargeting"/>
      </SubFeatures>
      <SubFeatures
          xsi:type="Exclusive"
          name="AdvancedTargeting">
        <SubFeatures
            xsi:type="Exclusive"
            name="Log">
          <SubFeatures
              name="PatternMatching"
              optional="true"/>
          <SubFeatures
              name="SymbolicPatterMatcher"
              optional="true"/>
          <SubFeatures
              name="PatternMatchingGun"
              optional="true"/>
        </SubFeatures>
        <SubFeatures
            xsi:type="Exclusive"
            name="BulletShoelding"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="AreaTargeting"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="Statistical">
          <SubFeatures
              xsi:type="Alternative"
              name="DistanceSegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="AngularTargeting"/>
          <SubFeatures
              xsi:type="Alternative"
              name="BulletFlightTimeSegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="BulletPowerSegementation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="EnemyVelocitySegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="TargetLiteralVelocitySegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="TargetAdvancedVelocitySegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="TargetAccelerationSegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="TimeSinceVelocityChangedSegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="TimeSinceVelocity0Segmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="TargetHeadingSegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="TimeSinceVelocityMaxSegmentation"/>
          <SubFeatures
              xsi:type="Alternative"
              name="EnemyRemainingSegmentation"/>
        </SubFeatures>
      </SubFeatures>
    </SubFeatures>
  </root>
</Feature_model>
