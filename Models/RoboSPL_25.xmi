<?xml version="1.0" encoding="UTF-8"?>
<Feature_model
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="https://fr.wikipedia.org/wiki/Feature_model"
    xsi:schemaLocation="https://fr.wikipedia.org/wiki/Feature_model ../MetaModels/Feature_MM.ecore">
  <root name="Module">
    <SubFeatures
        name="Radar">
      <SubFeatures
          xsi:type="Exclusive"
          name="SpinningRadar"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="TheInfinityLockRadar"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="WidthLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="TurnMultiplierLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="LazyLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="SpotlightLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="FancyRadar"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="FactorLock"/>
      <SubFeatures
          xsi:type="Exclusive"
          name="InteractiveRadar">
        <SubFeatures
            name="MouseRadar"
            optional="true"/>
      </SubFeatures>
    </SubFeatures>
    <SubFeatures
        name="Movement">
      <SubFeatures
          xsi:type="Exclusive"
          name="BasicMovement">
        <SubFeatures
            xsi:type="Exclusive"
            name="WallsMovement">
          <SubFeatures
              xsi:type="Exclusive"
              name="WallsMovementWithWallSmoothing"/>
          <SubFeatures
              xsi:type="Exclusive"
              name="BasicWallsMovement"/>
        </SubFeatures>
        <SubFeatures
            xsi:type="Exclusive"
            name="RandomMovement">
          <SubFeatures
              xsi:type="Exclusive"
              name="BasicRandomMovement"/>
          <SubFeatures
              xsi:type="Exclusive"
              name="TimeBasedRandomMovement"/>
        </SubFeatures>
      </SubFeatures>
      <SubFeatures
          xsi:type="Exclusive"
          name="AdvancedMovement"/>
    </SubFeatures>
    <SubFeatures
        name="Targeting">
      <SubFeatures
          xsi:type="Exclusive"
          name="BasicTargeting">
        <SubFeatures
            xsi:type="Exclusive"
            name="HeadOnTargeting"/>
        <SubFeatures
            xsi:type="Exclusive"
            name="LinearTargeting"/>
      </SubFeatures>
    </SubFeatures>
  </root>
</Feature_model>
